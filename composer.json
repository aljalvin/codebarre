{
    "name": "aljalvin/codebarre",
    "description": "generation des code barre 1D et 2D, Qr Code, PDF417, C39, C39+, C39E, C39E+, C93, S25, S25+, I25, I25+, C128, C128A, C128B, C128C, 2-Digits UPC-Based Extention, 5-Digits UPC-Based Extention, EAN 8, EAN 13, UPC-A, UPC-E, MSI (Variation of Plessey code)",
    "keywords": [
        "laravel",
        "Code Barre",
        "PDF417",
        "C39",
        "C39+",
        "C39E",
        "C39E+",
        "C93",
        "S25",
        "S25+",
        "I25",
        "I25+",
        "C128",
        "C128A",
        "C128B",
        "C128C",
        "2-Digits UPC-Based Extention",
        "5-Digits UPC-Based Extention",
        "EAN 8",
        "EAN 13",
        "UPC-A",
        "UPC-E",
        "MSI (Variation of Plessey code)"
    ],
    "minimum-stability": "stable",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "ALJALVIN Philippe",
            "email": "philourft@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 | ^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "aljalvin\\codebarre\\": "aljalvin/codebarre/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "aljalvin\\codebarre\\Provider\\BarcodeServiceProvider"
            ],
            "aliases": {
                "CODEBARRE": "aljalvin\\codebarre\\src\\Facades\\CODEBARRE"
            }
        }
    }
}
